{
  "name": "genaistudio-react",
  "version": "1.0.0",
  "description": "GenAI Studio - AEO Optimized React Website",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "react-snap": "^1.23.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && react-snap",
